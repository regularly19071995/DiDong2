        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DienThoaiActivity" />
        <activity android:name=".LaptopActivity" />
        <activity android:name=".LienHeActivity" />
        <activity android:name=".ChiTietSanPham" />
        <activity android:name=".Shopping_CartActivity" />
        <activity android:name=".ThongTinKHActivity" />

        <activity android:name=".InfoActivity"></activity>
    </application>

</manifest>